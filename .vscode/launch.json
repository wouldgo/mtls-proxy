  "version": "0.2.0",
  "configurations": [
    {
      "name": "proxy",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "cwd": "${workspaceFolder}",
      "program": "cmd/proxy",
      "env": {
	      "PEM_CREDS_FOLDER": "_pems",
        "PEM_FULLCHAIN": "ca.pem",
        "PEM_PRIVATE_KEY": "private_key.pem",
        "CERTS_FOLDER": "_fake_pki",
      }
    }
  ]
}
